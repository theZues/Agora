  <body>

    <header id="header" class="sixteen columns module">
      <div class="bg transition"></div>
      <div id="logo"><span>Agora</span></div>
      <nav>
        <ul class="eight columns">
          <li class="scroll transition"><a href="/Project2">Home</a></li>

          <li class="scroll transition"><a href="/">Contact</a></li>
        </ul>
      </nav>
    </header>

    <br class="clear" />


    <div class="container">

    <div class="sixteen columns module">
      <div class="container">

      <div class="sixteen columns module">
        <h2 class="fade1" "fade-in">Registered Users Area</h2>
        <br/>
        <h3>I have here some products that I am going to be putting up on eBay soon.  So because your registered, you can play around with these products or create your own listing.</h3>

      </div> <!-- Banner Image-->


    <br/>

    <%
      for(i=0; i < upcomingProducts.length; i++) { %>
        <img src="<%=upcomingProducts[i].img%>" alt="Image"/>
        <p><%=upcomingProducts[i].name%></p>
          <a href="/Project2/<%=upcomingProducts[i].id%>/edit">Edit</a>
          <form action="/Project2/new/<%=upcomingProducts[i].id%>?_method=DELETE" method="post">
            <input type="submit" value="Delete"/>
          </form>
        </br>
    <% } %>
    <br>

    <h2 id="createformtitle">Create your own here!</h2>
    <form action="/Project2/new/" method="POST">
      <input type="text" name="img" placeholder="image address"/></br>
      <input type="text" name="name" placeholder="name of item"/></br>
      <input type="text" name="details" placeholder="details"/></br>
      <input type="submit" name="" value="Create"/>
    </form>
  </div>
